            target = { target, features }: ((target."family" == "unix") && (!(target."os" == "macos")));
          }
          {
            name = "gtk-sys";
            packageId = "gtk-sys";
            target = { target, features }: ((target."family" == "unix") && (!(target."os" == "macos")));
          }
          {
            name = "javascriptcore-rs-sys";
            packageId = "javascriptcore-rs-sys";
            target = { target, features }: ((target."family" == "unix") && (!(target."os" == "macos")));
          }
          {
            name = "libc";
            packageId = "libc";
