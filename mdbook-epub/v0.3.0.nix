        resolvedDefaultFeatures = [ "bzip2" "default" "deflate" "flate2" ];
      };
      "zip 0.5.2" = rec {
        crateName = "zip";
        version = "0.5.2";
        edition = "2015";
        sha256 = "1hyv2hvqslrr78dayzw70i9lhbpp4yl5wy5c8rp062grz8hc73y1";
        authors = [
          "Mathijs van de Nes <git@mathijs.vd-nes.nl>"
        ];
        dependencies = [
          {
            name = "bzip2";
            packageId = "bzip2";
            optional = true;
