      };
      "xml-rs" = rec {
        crateName = "xml-rs";
        version = "0.8.0";
        edition = "2015";
        crateBin = [];
        sha256 = "1db4v716rbpgjiasaim2s17rmvsfcq1qzwg6nji6mdf5k34i46sl";
        libName = "xml";
        authors = [
          "Vladimir Matveev <vladimir.matweev@gmail.com>"
        ];
        
      };
      "zip 0.4.2" = rec {
        crateName = "zip";
        version = "0.4.2";
        edition = "2015";
        sha256 = "08p85wb1yg1ac4b96qf6z3sa51zbfcjlh7law3vmr9hqnn7y1f9n";
        authors = [
          "Mathijs van de Nes <git@mathijs.vd-nes.nl>"
