        version = "0.3.6";
        edition = "2015";
        sha256 = "1c6479gvfz8iqnrlicqcn5b5rgjpmnn241aa47x6q9qychryphcj";
        authors = [
          "Peter Atashian <retep998@gmail.com>"
        ];
        dependencies = [
          {
