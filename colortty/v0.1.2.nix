        resolvedDefaultFeatures = [ "dbghelp" "errhandlingapi" "fileapi" "handleapi" "lmcons" "minschannel" "minwindef" "ntsecapi" "processthreadsapi" "profileapi" "schannel" "securitybaseapi" "sspi" "std" "sysinfoapi" "timezoneapi" "winbase" "wincrypt" "winerror" "winnt" "winsock2" "ws2def" "ws2ipdef" "ws2tcpip" ];
