        resolvedDefaultFeatures = [ "basetsd" "consoleapi" "dbghelp" "errhandlingapi" "fileapi" "handleapi" "memoryapi" "minwinbase" "minwindef" "ntsecapi" "processenv" "processthreadsapi" "profileapi" "std" "sysinfoapi" "winbase" "winerror" "winnt" ];
