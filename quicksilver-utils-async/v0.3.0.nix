        resolvedDefaultFeatures = [ "handleapi" "libloaderapi" "lmcons" "minschannel" "minwindef" "ntsecapi" "processthreadsapi" "schannel" "securitybaseapi" "sspi" "sysinfoapi" "timezoneapi" "winbase" "wincrypt" "winerror" "winsock2" "ws2def" "ws2ipdef" "ws2tcpip" "wtypesbase" ];
