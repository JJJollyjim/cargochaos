      };
      "winapi-x86_64-pc-windows-gnu" = rec {
        crateName = "winapi-x86_64-pc-windows-gnu";
        version = "0.4.0";
        edition = "2015";
        sha256 = "0gqq64czqb64kskjryj8isp62m2sgvx25yyj3kpc2myh85w24bki";
        authors = [
          "Peter Atashian <retep998@gmail.com>"
        ];
        
      };
      "xdg-basedir" = rec {
        crateName = "xdg-basedir";
        version = "1.0.0";
        edition = "2015";
        sha256 = "1z5vzk6a7qxinqn3q70wzkh9y2ngjigb8y56dv3rskwwjdvd1580";
        libName = "xdg_basedir";
        authors = [
          "Kiran Purushothaman <projects@kirankp.com>"
        ];
        features = {
        };
