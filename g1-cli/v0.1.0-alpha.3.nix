          "The wasm-bindgen Developers"
        ];
        
      };
      "wasm-bindgen-webidl" = rec {
        crateName = "wasm-bindgen-webidl";
        version = "0.2.55";
        edition = "2018";
        sha256 = "1dab46bykgh8fzcg8m44h5r4igwzghwawjamzg41gdyffij3a9ii";
        authors = [
