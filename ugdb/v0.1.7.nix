      "xml-rs" = rec {
        crateName = "xml-rs";
        version = "0.7.0";
        edition = "2015";
        crateBin = [];
        sha256 = "1hp9kf80y9qm3aiqg5psyshqfkcrjgifbcm2c2nc5qlzs80vc71w";
        libName = "xml";
        authors = [
          "Vladimir Matveev <vladimir.matweev@gmail.com>"
